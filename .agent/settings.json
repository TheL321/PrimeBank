{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/build/**": true,
        "**/.gradle/**": true
    },
    "java.server.launchMode": "LightWeight",
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-8-openjdk",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/java-8-openjdk",
            "default": true
        }
    ]
}
